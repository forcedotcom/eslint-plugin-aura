{
  "name": "@salesforce/eslint-plugin-aura",
  "version": "1.4.0",
  "description": "ESLint plugin for Aura",
  "main": "lib/index.js",
  "repository": "https://github.com/forcedotcom/eslint-plugin-aura",
  "author": "Marat Vyshegorodtsev <marat@salesforce.com>",
  "contributors": [
    {
      "name": "Julien Roche",
      "email": "roche.jul@gmail.com",
      "url": "https://github.com/rochejul"
    }
  ],
  "keywords": [
    "eslint",
    "eslintplugin"
  ],
  "license": "BSD-3-Clause",
  "scripts": {
    "prepare": "husky install",
    "lint": "eslint lib/ test/",
    "format": "prettier --write \"**/*.{js,md}\"",
    "test": "mocha"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "eslint": "^7.22.0",
    "husky": "^5.2.0",
    "lint-staged": "^10.5.4",
    "mocha": "^8.3.2",
    "prettier": "^2.2.1",
    "proxyquire": "^2.1.3"
  },
  "peerDependencies": {
    "eslint": ">=5.0.0"
  },
  "dependencies": {
    "eslint-plugin-compat": "^3.9.0"
  },
  "engines": {
    "node": ">=8.0.0",
    "npm": ">=5.0.0"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "lint-staged": {
    "**/*.js": "eslint",
    "**/*.{js,md}": "prettier"
  },
  "prettier": {
    "singleQuote": true
  },
  "mocha": {
    "recursive": true,
    "reporter": "dot"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "git-precommit-checks && lint-staged"
    }
  }
}
